{
  "dependencies": {
    "@slack/web-api": "^7.14.1",
    "@slack/webhook": "^7.0.7",
    "commander": "^14.0.3",
    "https-proxy-agent": "^7.0.6",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@playwright/test": "^1.58.2",
    "@slack/types": "^2.20.0",
    "@types/node": ">=25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "dotenv": "^17.3.1",
    "eslint": "^10.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-playwright": "^2.5.1",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "nyc": "^17.1.0",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "ts-sinon": "^2.0.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "scripts": {
    "prettier": "prettier --write --loglevel warn \"**/**/*.ts\"",
    "pw": "nyc playwright test && nyc report --reporter=lcov",
    "build": "tsc -p ./tsconfig.json",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "cli": "yarn build && node dist/cli.js",
    "cli-debug": "yarn build && npx . -c ./cli_config.json -j ./tests/test_data/valid_test_results.json"
  },
  "name": "playwright-slack-report",
  "version": "1.1.102",
  "bin": {
    "playwright-slack-report": "dist/cli.js"
  },
  "main": "dist/cli.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:ryanrosello-og/playwright-slack-report.git",
  "author": "Ryan Rosello <ryanrosello@hotmail.com>",
  "license": "MIT",
  "files": [
    "/dist/src"
  ],
  "keywords": [
    "slack",
    "report",
    "playwright",
    "typescript"
  ],
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "./custom_block/my_block.ts"
    ]
  }
}
